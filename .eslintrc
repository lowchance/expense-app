{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
		"linebreak-style":"off",
		"no-tabs":"off",
		"no-case-declarations":0,
        "no-unused-vars":[
            "error",
                {
                    "vars":"local",
                    "args":"none"
                }
        ],
        "indent": ["error", "tab"],
		"react/jsx-filename-extension": 0,
		"react/jsx-indent": 0,
		"react/jsx-indent-props": 0,
		"import/extensions":0,
		"arrow-parens": 0,
		"no-else-return": 0
    }
}